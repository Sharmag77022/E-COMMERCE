<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Merchant Requests</title>
        <link rel="icon"  type="image/png" href="/websiteLogo.png">
        <link rel="stylesheet" href="/css/bootstrap.css">
        <script src="/js/jquery-3.5.1.min.js"></script>
        <script src="/js/bootstrap.js"></script>
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <script src='/js/loadMenu.js'></script> 
         <link href="https://fonts.googleapis.com/css?family=Oswald" rel=“stylesheet”>
    
    </head>

    <body >
        <div id="includedContent"></div>        
        <div class="card">
            <div class="card-body text-danger font-weight-bold bg-warning">
             <h2> Merchant Requests</h2>
            </div>
          </div>
        <br>
        <table class="table table-dark" id="merchantRequests" >
            <tr><td>Serial Number</td>
                <td>Name</td>
                <td>Email</td>
                <td>Accept</td>
                <td>Reject</td></tr>
          </table><br>
          <div class="card">
            <div class="card-body text-danger font-weight-bold bg-warning">
             <h2> Merchant Category Requests</h2>
          </div></div><br>
            <table class="table table-dark" id="categoryRequests" >
                <tr><td>Serial Number</td>
                    <td>Category Name</td>
                    <td>Merchant Id</td>
                    <td>Accept</td>
                    <td>Reject</td></tr>
            </table>
          </div>
          <!-- Merchant Sub Categories Requests Section Start-->
          <div class="card">
            <div class="card-body text-danger font-weight-bold bg-warning">
             <h2> Merchant Sub Category Requests</h2>
            </div>
          </div>
        <br>
        <table class="table table-dark" id="subCatRequests" >
            <tr><td>Serial Number</td>
                <td>Category Name</td>
                <td>Merchant ID</td>
                <td>Parent Category</td>
                <td>Accept</td>
                <td>Reject</td></tr>
          </table><br>
           <!-- Merchant Sub Categories Requests Section End-->
          <script src="/js/merchantRequests.js"></script>

    </body>
</html>